{
  "page_type": "dashboard",
  "title": "Analytics Dashboard",
  "sections": [
    {
      "id": "nav",
      "type": "navbar",
      "components": [
        {"type": "logo"},
        {"type": "search"},
        {"type": "profile"}
      ]
    },
    {
      "id": "main",
      "type": "main",
      "layout": {
        "grid": "12-col",
        "breakpoints": {"sm": 1, "md": 6, "lg": 12}
